##
## 開発環境用設定ファイル
##

# JDBC接続ドライバクラス(DataSourceを直接使用する際の項目)
nablarch.db.jdbcDriver=org.h2.jdbcx.JdbcDataSource

# JDBC接続URL(DataSourceを直接使用する際の項目)
nablarch.db.url=jdbc:h2:./h2/db/nablarch_example

# DB接続ユーザ名(DataSourceを直接使用する際の項目)
nablarch.db.user=NABLARCH_EXAMPLE

# DB接続パスワード(DataSourceを直接使用する際の項目)
nablarch.db.password=NABLARCH_EXAMPLE

# DBスキーマ名(DataSourceを直接使用する際の項目)
nablarch.db.schema=PUBLIC

# 最大プールサイズ(DataSourceを直接使用する際の項目)
nablarch.db.maxPoolSize=10

# 初期プールサイズ(DataSourceを直接使用する際の項目)
nablarch.db.initialPoolSize=2


# 以下、郵便番号登録ETLバッチで使用する設定項目
# 接続するデータベース名（SQL*Loaderを使用する際の項目）
db.databaseName=xe

# 接続するユーザ名（SQL*Loaderを使用する際の項目）
db.user=NABLARCH_EXAMPLE

# ユーザのパスワード（SQL*Loaderを使用する際の項目）
db.password=NABLARCH_EXAMPLE